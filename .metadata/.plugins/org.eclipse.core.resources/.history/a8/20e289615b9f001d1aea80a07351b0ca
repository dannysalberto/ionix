package com.colsubsidio.purchaseorders.exceptions;

public class DispensaryNotFoundException  extends RuntimeException {
	
	 /**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	public DispensaryNotFoundException(){
	        super("Dispensario no existe");
	    }

	    public DispensaryNotFoundException(String message) {
	        super(message);
	    }

	    public DispensaryNotFoundException(Throwable cause) {
	        super(cause);
	    }

	    public DispensaryNotFoundException(String message, Throwable cause, boolean enableSuppression, boolean writableStackTrace) {
	        super(message, cause, enableSuppression, writableStackTrace);
	    }

}
